%start S

S[SEM=<Poignarde(?vp)>] -> VP[SEM=?vp]
VP[SEM=?n] -> N[SEM=?n] V AD
N[SEM=?np] -> Nprop[SEM=?np]
V -> 'a' | 'est'
AD -> 'mort' | 'morte' | 'egorge'

Nprop[SEM=<Ben>] -> 'Ben'
Nprop[SEM=<Alec>] -> 'Alec'
Nprop[SEM=<Gab>] -> 'Gab'
Nprop[SEM=<Pei>] -> 'Pei'
Nprop[SEM=<Sylvie>] -> 'Sylvie'
Nprop[SEM=<Eric>] -> 'Eric'